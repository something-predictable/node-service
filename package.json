{
  "name": "@riddance/service",
  "version": "0.1.2",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "riddance",
    "serverless",
    "environment",
    "ioc",
    "devops",
    "devenv",
    "opinionated"
  ],
  "files": [
    "./*.js",
    "./*.d.ts",
    "./lib/*.js",
    "./mock/*.js",
    "./mock/*.d.ts"
  ],
  "sideEffects": [
    "./context.js",
    "./http.js"
  ],
  "exports": {
    "./context": "./context.js",
    "./http": "./http.js",
    "./test/context": "./mock/context.js",
    "./test/http": "./mock/http.js"
  },
  "dependencies": {
    "@riddance/host": "0.1.2",
    "jsonwebtoken": "9.0.2"
  },
  "devDependencies": {
    "@riddance/env": "0.6.4",
    "@types/jsonwebtoken": "9.0.7"
  },
  "scripts": {
    "start": "riddance-watch",
    "prepack": "riddance-build",
    "prepublishOnly": "riddance-prepublish",
    "postpublish": "riddance-sync"
  }
}
