{
  "name": "@riddance/service",
  "version": "0.2.5",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "keywords": [
    "riddance",
    "serverless",
    "environment",
    "ioc",
    "devops",
    "devenv",
    "opinionated"
  ],
  "files": [
    "./*.js",
    "./*.d.ts",
    "./lib/*.js",
    "./mock/*.js",
    "./mock/*.d.ts",
    "instructions.md"
  ],
  "sideEffects": [
    "./context.js",
    "./http.js",
    "./timer.js",
    "./event.js"
  ],
  "exports": {
    "./context": "./context.js",
    "./http": "./http.js",
    "./timer": "./timer.js",
    "./event": "./event.js",
    "./test/context": "./mock/context.js",
    "./test/http": "./mock/http.js",
    "./test/timer": "./mock/timer.js",
    "./test/event": "./mock/event.js"
  },
  "ai": {
    "instructions": "./instructions.md"
  },
  "dependencies": {
    "@riddance/host": "0.2.4",
    "cron": "4.3.3",
    "jose": "6.1.0"
  },
  "devDependencies": {
    "@riddance/env": "0.9.3"
  },
  "scripts": {
    "start": "riddance-watch",
    "prepack": "riddance-build",
    "prepublishOnly": "riddance-prepublish",
    "postpublish": "riddance-sync"
  }
}
