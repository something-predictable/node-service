{
  "name": "@riddance/service",
  "version": "0.2.0",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "keywords": [
    "riddance",
    "serverless",
    "environment",
    "ioc",
    "devops",
    "devenv",
    "opinionated"
  ],
  "files": [
    "./*.js",
    "./*.d.ts",
    "./lib/*.js",
    "./mock/*.js",
    "./mock/*.d.ts"
  ],
  "sideEffects": [
    "./context.js",
    "./http.js",
    "./timer.js",
    "./event.js"
  ],
  "exports": {
    "./context": "./context.js",
    "./http": "./http.js",
    "./timer": "./timer.js",
    "./event": "./event.js",
    "./test/context": "./mock/context.js",
    "./test/http": "./mock/http.js",
    "./test/timer": "./mock/timer.js",
    "./test/event": "./mock/event.js"
  },
  "dependencies": {
    "@riddance/host": "0.2.0",
    "cron": "4.3.3",
    "jose": "6.0.12"
  },
  "devDependencies": {
    "@riddance/env": "0.8.2"
  },
  "scripts": {
    "start": "riddance-watch",
    "prepack": "riddance-build",
    "prepublishOnly": "riddance-prepublish",
    "postpublish": "riddance-sync"
  }
}
